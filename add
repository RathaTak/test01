// add
Public int add(int a, int b,  int c) {
// retour valeur
return retourneValeur();
}

// Set value 1
private int value1 = "";
public void setValue1(int val) {
value1 = val;
}

// Set value 2
private int value2 = "";
public void setValue2(int val) {
value2 = val;
}

// calculer valeur et retourner valeur
Public int retourneValeur() {
int res = setValue1(1) + setValue2(2);
return res;
}
