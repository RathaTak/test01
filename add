// add
Public int add(int a, int b,  int c) {
// retour valeur
return a + b + c;
}

// Set value 1
private int value1 = "";
public void setValue1(int val) {
value1 = val;
}

// Set value 2
private int value2 = "";
public void setValue2(int val) {
value2 = val;
}

// Set value 3
private int value3 = "";
public void setValue3(int val) {
value3 = val;
}

// calcul valeur
Public int retourneValeur() {
int res = setValue1(1) + setValue2(2) + setValue3(3)
return res;
}
